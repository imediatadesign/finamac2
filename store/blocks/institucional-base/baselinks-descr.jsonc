                            "props": {
                                "itemsPerPage": {
                                "desktop": 3,
                                "tablet": 1,
                                "phone": 1
                                },
                                "infinite": true,
                                "showNavigationArrows": "always",
                                "showPaginationDots":"never",
                                "fullWidth":true
                            }
                        },
                    

