                "video#baselinks-descr-2": {
                    "props": {
                    "height": "450px",
                    "loop": true,
                    "autoPlay": false,
                    "muted": false,
                    "src": "https://youtu.be/QKdpAYsJpD4",
                    "blockClass":"baselinks-descr-2"
