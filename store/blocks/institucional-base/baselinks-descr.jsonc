    // Row 1

    "toggle-layout#baselinks-descr-1": {
        "children": [
            "flex-layout.row#baselinks-descr-1"
          ]
      },

        "flex-layout.row#baselinks-descr-1": {
            "props": {
                "blockClass": [
                    "1300",
                    "base2",
                    "padding40vs20"
                ],
                "rowGap":4
            },
            "children": [
                    "rich-text#baselinks-descr-title"
            ]
        },
            "rich-text#baselinks-descr-title": {
                "props": {
                    "text": "Clientes Finamac\n### Título Principal\n\nFaça mais com as máquinas Finamac™",
                    "textAlignment": "CENTER",
                    "textPosition": "CENTER",
                    "blockClass":"color-primary"
                }
            },

    // Row 2 VIDEO

    "toggle-layout#baselinks-descr-video-2": {
        "children": [
            "flex-layout.row#baselinks-descr-video-2"
          ]
