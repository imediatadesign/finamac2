      "flex-layout.row#link-drawer-es"
    ]
  },

  // LOGO

    "flex-layout.row#logo-drawer": {
        "props": {
          "paddingBottom":7,
          "preventHorizontalStretch":true,
          "horizontalAlign":"left"
        },
        "children": [
          "image#logo-drawer"
        ]
      },

      "image#logo-drawer": {
        "props": {
          "src": "https://finamac.vtexassets.com/assets/vtex.file-manager-graphql/images/a76c86e6-31a9-44e8-91a2-ad5554fe289c___739bd23f50db5ff9df29d41b33c0a855.svg",
          "width": "180px"
        }
    },


  

  // Link Drawer

  "flex-layout.row#link-drawer": {
    "props": {
      "blockClass": "link-drawer",
      "preventHorizontalStretch":true
    },
    "children": [
      "flex-layout.col#link-drawer-1"
    //  "flex-layout.col#link-drawer-2"
    ]
  },

  // Link 1-1
  
  "flex-layout.col#link-drawer-1": {
    "props": {
      "blockClass": "link-drawer-1",
