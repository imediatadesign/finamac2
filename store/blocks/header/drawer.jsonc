{
  // Menu Drawer
  // PRA RESOLVER BUG SANFONA, ENTRE NO ADMIN E TRAGA A CATEGORIA NOVAMENTE
  // ENTRAR NO MENU VIA EDITOR, E COLOCAR O ID DA CATEGPORIA LA DENTRO, 

  // ========== DRAWER MULTI-IDIOMA ==========
  "drawer#menu": {
    "props": {},
    "children": [
      "condition-layout.binding#drawer-switcher"
    ]
  },

  "condition-layout.binding#drawer-switcher": {
    "props": {
      "conditions": [{
        "subject": "bindingId",
        "arguments": {
          "id": "33cd2dcc-c319-4da0-82d5-71aa38b19380"  // PT-BR
        }
      }],
      "Then": "menu#drawer",
      "Else": "condition-layout.binding#drawer-en-es"
    }
  },

  "condition-layout.binding#drawer-en-es": {
    "props": {
      "conditions": [{
        "subject": "bindingId",
        "arguments": {
          "id": "56fe0518-7750-4e50-ada7-9d9c7e2f41c8"  // EN-US
        }
      }],
      "Then": "menu#drawer-en",
      "Else": "menu#drawer-es"  // ES-ES fallback
    }
  },

  // DRAWER PT-BR
  "menu#drawer": {
    "children": [
      "flex-layout.col#menu-drawer"
    ],
    "props": {
      "blockClass": "menu-drawer"
    }
  },
  "flex-layout.col#menu-drawer": {
    "props": {},
    "children": [
      "flex-layout.row#logo-drawer",
      "flex-layout.row#drawer-menu-all-products",
      "vtex.menu@2.x:menu#footer-mobile_sanfone-category",
      "flex-layout.row#drawer-menu-title",
     "flex-layout.row#link-drawer"
    ]
  },

  // DRAWER EN-US
  "menu#drawer-en": {
    "children": [
      "flex-layout.col#menu-drawer-en"
    ],
    "props": {
      "blockClass": "menu-drawer"
    }
  },
  "flex-layout.col#menu-drawer-en": {
    "props": {},
    "children": [
      "flex-layout.row#logo-drawer",
      "flex-layout.row#drawer-menu-all-products-en",
      "vtex.menu@2.x:menu#footer-mobile_sanfone-category-en",
      "flex-layout.row#drawer-menu-title",
      "flex-layout.row#link-drawer-en"
    ]
  },

  // DRAWER ES-ES
  "menu#drawer-es": {
    "children": [
      "flex-layout.col#menu-drawer-es"
    ],
    "props": {
      "blockClass": "menu-drawer"
