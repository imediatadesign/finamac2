  // Menu Drawer
  // PRA RESOLVER BUG SANFONA, ENTRE NO ADMIN E TRAGA A CATEGORIA NOVAMENTE
  // ENTRAR NO MENU VIA EDITOR, E COLOCAR O ID DA CATEGPORIA LA DENTRO, 

  "drawer#menu": {
    "props": {
      // "maxWidth": "100%", 
      // "slideDirection":"vertical",
      // "isFullWidth":true
    },
    "children": [
      "menu#drawer"
    ]
  },
  "menu#drawer": {
    "children": [
      "flex-layout.col#menu-drawer"
    ],
    "props": {
      "blockClass": "menu-drawer"
    }
  },
  "flex-layout.col#menu-drawer": {
    "props": {},
    "children": [
      /*"authentication-layout-private#home",
      "authentication-layout-public#home",*/
      "flex-layout.row#logo-drawer",
      "flex-layout.row#drawer-menu-title-products",
     "vtex.menu@2.x:menu#footer-mobile_sanfone-category",
      "flex-layout.row#drawer-menu-title",
     "flex-layout.row#link-drawer"
    ]
  },

  // LOGO

    "flex-layout.row#logo-drawer": {
        "props": {
          "paddingBottom":7,
          "preventHorizontalStretch":true,
          "horizontalAlign":"left"
        },
        "children": [
          "image#logo-drawer"
        ]
      },

      "image#logo-drawer": {
        "props": {
          "src": "https://finamac.vtexassets.com/assets/vtex.file-manager-graphql/images/a76c86e6-31a9-44e8-91a2-ad5554fe289c___739bd23f50db5ff9df29d41b33c0a855.svg",
          "width": "180px"
        }
    },


  

  // Link Drawer

  "flex-layout.row#link-drawer": {
    "props": {
      "blockClass": "link-drawer",
      "preventHorizontalStretch":true
    },
    "children": [
      "flex-layout.col#link-drawer-1"
    //  "flex-layout.col#link-drawer-2"
    ]
  },

  // Link 1-1
  
  "flex-layout.col#link-drawer-1": {
    "props": {
      "blockClass": "link-drawer-1",
      "width":"100%"
    },
    "children": [
      "link#drawer-link-1-1"
    ]
  },

  "link#drawer-link-1-1": {
    "children": [
      "image#drawer-link-1-1"
    ],
    "props": {
      "href": "/contato",
      "label": "FALE COM NOSSO TIME",
      "blockClass": "link-drawer",
      "displayMode":"button"
    }
  },
      "image#drawer-link-1-1": {
        "props": {
          "src": "data:image/svg+xml;base64,",
          "blockClass": "drawer-link"
        }
      },

  // Link 1-2

  "flex-layout.col#link-drawer-2": {
    "props": {
      "blockClass": "link-drawer-2",
      "width":"100%"
    },
    "children": [
      "link#drawer-link-1-2"
    ]
  },
      "link#drawer-link-1-2": {
        "children": [
          "image#drawer-link-1-2"
        ],
        "props": {
          "href": "/lancamentos",
          "label": "Nossos Lan√ßamentos",
          "blockClass": "link-drawer"
        }
      },
      "image#drawer-link-1-2": {
        "props": {
          "src": "https://polpabrasil.vtexassets.com/assets/vtex.file-manager-graphql/images/131ebe67-ef06-4a67-9d6b-b72165cf97d2___6cb4b501628762a845cf5733c38492c8.svg",
          "blockClass": "drawer-link"
        }
      },


  // Title Menu
  "flex-layout.row#drawer-menu-title": {
    "props": {
      "blockClass": "drawer-menu-title"
    },
    "children": [
      "rich-text#drawer-menu-title"
    ]
  },
  "rich-text#drawer-menu-title": {
    "props": {
      "text": "Atalhos",
      "blockClass": "drawer-menu-title"
    }
  },
  // Titile Products
  "flex-layout.row#drawer-menu-title-products": {
    "props": {
      "blockClass": "drawer-menu-title"
    },
    "children": [
      "rich-text#drawer-menu-title-products"
    ]
  },
  "rich-text#drawer-menu-title-products": {
    "props": {
      "text": "[Produtos](/produtos)",
      "blockClass": "drawer-menu-title"
    }
  }

