  /* Search Bar */
  "flex-layout.col#search-bar-overlay": {
    "props": {
      "verticalAlign": "middle",
      "preventVerticalStretch": true,
      "width": "auto"
    },
    "children": [
      "condition-layout.binding#search-bar"
    ]
  },
  "condition-layout.binding#search-bar": {
    "props": {
      "conditions": [
        {
          "subject": "bindingId",
          "arguments": { "id": "33cd2dcc-c319-4da0-82d5-71aa38b19380" },
          "toBe": { "type": "equals" }
        },
        {
          "subject": "bindingId",
          "arguments": { "id": "1fe4f6bc-dc38-44c6-b840-b330a71693df" },
          "toBe": { "type": "equals" }
        }
      ]
    },
    "children": [
      "overlay-trigger#search-bar",
      "overlay-trigger#search-bar-en",
      "overlay-trigger#search-bar-es"
    ]
  },
  "overlay-trigger#search-bar": {
    "props": {
      "trigger": "click",
      "blockClass": "search-bar"
    },
    "children": [
      "image#searchhome",
