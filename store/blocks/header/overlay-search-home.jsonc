{
  // Search 
  "flex-layout.row#search-bar": {
    "props": {
      "fullWidth": true,
      "blockClass": "search-bar"
    },
    // "children": ["search-bar"]
    "children": [
      "flex-layout.col#search-bar"
    ]
  },
  "flex-layout.col#search-bar": {
    "props": {
      "width": "15%",
      "preventVerticalStretch": true,
      "verticalAlign": "middle",
      "paddingRight": 0
    },
    "children": [
      "search-bar"
    ]
  },
  "search-bar": {
    "blocks": [
      "autocomplete-result-list.v2"
    ],
    "props": {
      // "openAutocompleteOnFocus": false,
      "openAutocompleteOnFocus": false,
      "autocompleteAlignment": "center",
      "autocompleteFullWidth": true,
      "autoFocus": false,
      "blurOnSubmit": true,
      "containerMode": "container",
      "disableBlurAndTouchEndHandler": true,
      "emptyPlaceholder": "Digite sua busca aqui",
      "hasIconLeft": false,
      "inputType": "text",
      // "maxWidth": 100,
      "minSearchTermLength": 3,
      "placeholder": "O que você está procurando?",
      "displayMode": "search-button"
    }
  },
  /* Search Bar */
  "flex-layout.col#search-bar-overlay": {
    "props": {
      "verticalAlign": "middle",
      "preventVerticalStretch": true,
      "width": "auto"
    },
    "children": [
      "overlay-trigger#search-bar"
    ]
  },
  "overlay-trigger#search-bar": {
    "props": {
      "trigger": "click",
      // "trigger": "hover",
      "blockClass": "search-bar"
    },
    "children": [
      "image#searchhome",
      "overlay-layout#search-bar"
    ]
  },
  "image#searchhome": {
    "props": {
      "src": "data:image/svg+xml;base64,",
      "width": "18px"
    }
  },
  // Content 
  "overlay-layout#search-bar": {
    "props": {
      "placement": "bottom",
      "scrollBehavior": "default",
      "blockClass": "overlay-search-bar",
      "showArrow": false,
      "offsets": {
        "distance": 20
      }
    },
    "children": [
      "flex-layout.col#search-bar-content"
    ]
  },
  "flex-layout.col#search-bar-content": {
    "props": {
      "width": "grow"
    },
    "children": [
      "search-bar#overlay"
    ]
  },
  "search-bar#overlay": {
    "blocks": [
      "autocomplete-result-list.v2"
    ],
    "props": {
      "openAutocompleteOnFocus": true,
      "autocompleteAlignment": "center",
      "autocompleteFullWidth": true,
      "autoFocus": true,
      "blurOnSubmit": true,
      "containerMode": "container",
      "disableBlurAndTouchEndHandler": true,
      "displayMode": "clear-button",
      "hasIconLeft": false,
      "inputType": "text",
      "minSearchTermLength": 3
    }
  },
  "autocomplete-result-list.v2": {
    "blocks": [
      "product-summary.shelf"
    ],
    "props": {
      "hideUnavailableItems": true,
      "maxSuggestedProducts": 2
      // "__unstableProductOriginVtex":false,
      // "customBreakpoints":"md"
    }
  }
}