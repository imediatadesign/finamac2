  // Container principal do Footer Info - ESPAÑOL
  "flex-layout.row#footer-info--es": {
    "props": {
      "blockClass": "footer-info-container",
      "fullWidth": true,
      "preserveLayoutOnMobile": true
    },
    "children": [
      "flex-layout.col#footer-info-content--es"
    ]
  },
  "flex-layout.col#footer-info-content--es": {
    "props": {
      "blockClass": "footer-info-main"
