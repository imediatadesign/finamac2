  // Coluna 1 - Sobre Finamac
  "flex-layout.col#footer-about--es": {
    "props": {
      "blockClass": "footer-column footer-about",
      "width": {
        "desktop": "25%",
        "mobile": "100%"
      }
    },
    "children": [
      "rich-text#footer-about-title--es",
      "rich-text#footer-about-description--es",
      "flex-layout.row#footer-social"
    ]
  },
  "rich-text#footer-about-title--es": {
    "props": {
      "text": "## FINAMAC",
      "blockClass": "footer-column-title",
      "textAlignment": "LEFT"
